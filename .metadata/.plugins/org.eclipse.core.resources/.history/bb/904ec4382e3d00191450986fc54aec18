package course;

import java.util.Scanner;

public class WhileFor {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();

		while (n <= 0) {
			System.out.println("entre com um numero positivo: ");
			n = sc.nextInt();
		}
		for (int i = 0; i < n; i++) {
			int h = sc.nextInt();
			if (h > h) {
				System.out.println(h);

			}
		}

		sc.close();
	}
}